    {
      "kind": "Title",
      "id": "OVPN Serpro",
      "matching_strategy": "Equals"
    }
  ],
  "animation": {
    "enabled": true,
    "duration": 300,
    "fps": 60,
    "style": "EaseOutSine"
  },
